<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dao.AIProductMapper">


    <select id="queryPriceById" parameterType="java.lang.Long" resultType="java.math.BigDecimal">
        select price
        from product
        where id = #{productId}
    </select>
</mapper>